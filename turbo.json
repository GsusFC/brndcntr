{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "lint": {
      "dependsOn": [
        "^lint"
      ]
    }
  },
  "globalEnv": [
    "INDEXER_DATABASE_URL",
    "NEXT_PUBLIC_REOWN_PROJECT_ID"
  ]
}